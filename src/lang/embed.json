{
	"mute.success": { "title": "$mute.success.title", "color": "$color.green" },
	"unmute.success": {
		"title": "$unmute.success.title",
		"color": "$color.green"
	},
	"unlock.success": {
		"title": "$unlock.success.title",
		"color": "$color.green"
	},
	"lock.success": { "title": "$lock.success.title", "color": "$color.green" },
	"kick.success": { "title": "$kick.success.title", "color": "$color.green" },
	"lock_duration.success": {
		"title": "$lock.success.title",
		"color": "$color.green",
		"fields": [{ "name": "Duration", "value": "$duration" }]
	},
	"error.invalid_duration": {
		"title": "$error.invalid_duration.title",
		"color": "$color.red",
		"options": { "ephemeral": true }
	},
	"mute.role_invalid": {
		"title": "$mute.role_invalid.title",
		"description": "$mute.role_invalid.description",
		"color": "$color.red",
		"options": { "ephemeral": true }
	},
	"kick.not_allowed": {
		"title": "$kick.not_allowed.title",
		"color": "$color.red",
		"options": { "ephemeral": true }
	},
	"error.bot_missing_permission": {
		"title": "$error.bot_missing_permission.title",
		"color": "$color.red",
		"options": { "ephemeral": true }
	},
	"error.user_missing_permission": {
		"title": "$error.user_missing_permission.title",
		"color": "$color.red",
		"options": { "ephemeral": true }
	}
}
